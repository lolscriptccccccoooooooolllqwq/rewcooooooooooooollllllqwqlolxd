-- 创建加载框的旋转动画
game:GetService("RunService").RenderStepped:Connect(function()

    local rotation = tick() * 180 % 360
    for i = 1, numDots do
        local angle = math.rad(i * (180 / numDots) + rotation)
        local x = radius * math.cos(angle)
        local y = radius * math.sin(angle)
        
        local dot = dots[i]
        
        dots[i].BackgroundColor3 = xaaakal
        dots[i].Position = UDim2.new(0.5 + x / radius, -4, 0.5 + y / radius, -4)
       
    end
   end)
   
   wait(math.random(5,15))
   --loadingFrame.Visible=false
   
   

   for i=1,numDots do
   dots[i] .Transparency=1
   wait(0.1)
   end

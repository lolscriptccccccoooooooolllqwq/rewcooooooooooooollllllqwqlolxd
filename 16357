local radius = 60
dots = {}
local timeElapsed = 0

for i=1,numDots do

            dots[i] = Instance.new("Frame")
            dots[i].Size = UDim2.new(0, 8, 0, 8)
            
          dots[i].BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            dots[i].Parent = loadingFrame
